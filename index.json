{
  "arrowParens": "always",
  "bracketSpacing": false,
  "htmlWhitespaceSensitivity": "ignore",
  "importOrder": ["^\\$lib", "^[./]"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "printWidth": 80,
  "proseWrap": "always",
  "quoteProps": "preserve",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 3,
  "trailingComma": "none",
  "useTabs": true
}
